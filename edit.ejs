<%- include('partials/head')%>

<h1>Edit Your Order</h1>
    <form method="POST" action="/coffees/<%=coffee.id%>?_method=PATCH">
        <label for="type">Type</label>
        <input type="text" id="type" name="type" value="<%=coffee.type%>">
        <label for="size">size</label>
        <input type="text" id="size" name="size" value="<%=coffee.size%>">
        <label for="drinker">drinker</label>
        <input type="text" id="drinker" name="drinker" value="<%=coffee.drinker%>">
        <label for="price">price</label>
        <input type="number" id="price" name="price" value="<%=coffee.price%>">
        <button class="btn btn-outline-warning">Update Order</button>
    </form>

<%- include('partials/foot')%>